<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LayoutRatio">
        <attr name="widthByHeight" format="float"/>
        <attr name="heightByWidth" format="float"/>
        <attr name="squareByWidth" format="boolean" />
        <attr name="squareByHeight" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>


</resources>